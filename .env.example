# Real Estate Video Pipeline - Environment Configuration

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/real_estate_pipeline
REDIS_URL=redis://localhost:6379/0

# AWS S3
AWS_VIDEO_API_ACCESS_KEY_ID=your_access_key
AWS_VIDEO_API_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=us-east-1
S3_BUCKET_VIDEOS=unpin-real-estate-videos
S3_BUCKET_RESULTS=unpin-real-estate-results

# RunPod
RUNPOD_API_KEY=your_runpod_api_key
RUNPOD_ENDPOINT_ID=your_endpoint_id

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=false

# Security
SECRET_KEY=your_secret_key_here
CORS_ORIGINS=http://localhost:3000,https://yourdomain.com

# Webhook URLs
WEBHOOK_BASE_URL=https://yourapi.com/webhook