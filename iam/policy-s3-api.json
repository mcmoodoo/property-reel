{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "FullVideoBucketAccess",
      "Effect": "Allow",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::unpin-real-estate-videos",
        "arn:aws:s3:::unpin-real-estate-videos/*"
      ]
    },
    {
      "Sid": "ReadResultsBucketAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::unpin-real-estate-results",
        "arn:aws:s3:::unpin-real-estate-results/*"
      ]
    }
  ]
}
